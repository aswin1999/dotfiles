{
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "p"],
      "after": ["o", "<Esc>", "p"]
    },
    {
      "before": ["<C-n>"],
      "after": [],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    },
    {
      "before": ["leader", "w"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": ["leader", "f"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatDocument",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "q", "q"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor"
        }
      ]
    },
    {
      "before": ["<CR>"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["w"],
      "after": ["<leader>", "<leader>", "w"]
    },
    {
      "before": ["b"],
      "after": ["<leader>", "<leader>", "b"]
    },
    {
      "before": ["e"],
      "after": ["<leader>", "<leader>", "l"]
    },
    {
      "before": ["r"],
      "after": ["<leader>", "<leader>", "h"]
    },
    {
      "before": ["<leader>", "d"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.previewDeclaration"
        }
      ]
    },
    {
      "before": ["J"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.previousEditor"
        }
      ]
    },
    {
      "before": ["K"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.nextEditor"
        }
      ]
    },
    {
      "before": ["H"],
      "after": ["0"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-k>": false
  },
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "gitlens.advanced.messages": {
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.currentLine.enabled": false,
  "zenMode.hideStatusBar": false,
  "terminal.integrated.shell.linux": "zsh",
  "prettier.eslintIntegration": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "explorer.openEditors.visible": 0,
  "git.enableSmartCommit": true,
  "zenMode.hideTabs": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "text.html.markdown.journal.task.open.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.open.marker",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.open.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.open.text",
        "settings": {}
      },
      {
        "scope": "text.html.markdown.journal.task.completed.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.completed.marker",
        "settings": {
          "foreground": "#AAAAAA"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.completed.text",
        "settings": {
          "foreground": "#AAAAAA"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.completed.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.memo.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.memo.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.scope",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.link.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.link.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      }
    ]
  },
  "terminal.explorerKind": "external",
  "vim.sneak": true,
  "workbench.colorTheme": "Monokai",
  "editor.fontLigatures": true,
  "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "git.defaultCloneDirectory": "~",
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "terminal.integrated.rendererType": "dom",
  "keyboard.dispatch": "keyCode",
  "terminal.integrated.copyOnSelection": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "prettier.tslintIntegration": true,
  "editor.formatOnSave": true,
  "workbench.activityBar.visible": true
}
